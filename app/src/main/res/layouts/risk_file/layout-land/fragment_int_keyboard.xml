<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controllers.navigationPackageA.riskTheme.IntKeyboardFragment">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/one_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/accentdark_border_table"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:tag="1"
            android:text="1"
            android:textColor="@color/background_light"
            android:textSize="30sp"
            app:layout_constraintBottom_toTopOf="@+id/six_btn"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toStartOf="@+id/two_btn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/two_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="@drawable/accentdark_border_table"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:text="2"
            android:tag="2"
            android:textColor="@color/background_light"
            android:textSize="30sp"
            app:layout_constraintBottom_toBottomOf="@+id/one_btn"
            app:layout_constraintEnd_toStartOf="@+id/three_btn"
            app:layout_constraintStart_toEndOf="@+id/one_btn"
            app:layout_constraintTop_toTopOf="@+id/one_btn" />

        <TextView
            android:id="@+id/three_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:background="@drawable/accentdark_border_table"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:tag="3"
            android:text="3"
            android:textColor="@color/background_light"
            android:textSize="30sp"
            app:layout_constraintBottom_toBottomOf="@+id/two_btn"
            app:layout_constraintEnd_toStartOf="@+id/four_btn"
            app:layout_constraintStart_toEndOf="@+id/two_btn"
            app:layout_constraintTop_toTopOf="@+id/two_btn" />

        <TextView
            android:id="@+id/four_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="@drawable/accentdark_border_table"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:tag="4"
            android:text="4"
            android:textColor="@color/background_light"
            android:textSize="30sp"
            app:layout_constraintBottom_toBottomOf="@+id/three_btn"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toStartOf="@+id/five_btn"
            app:layout_constraintStart_toEndOf="@+id/three_btn"
            app:layout_constraintTop_toTopOf="@+id/three_btn" />

        <TextView
            android:id="@+id/five_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:background="@drawable/accentdark_border_table"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:tag="5"
            android:text="5"
            android:textColor="@color/background_light"
            android:textSize="30sp"
            app:layout_constraintBottom_toBottomOf="@+id/four_btn"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/four_btn"
            app:layout_constraintTop_toTopOf="@+id/four_btn" />

        <TextView
            android:id="@+id/six_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/accentdark_border_table"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:tag="6"
            android:text="6"
            android:textColor="@color/background_light"
            android:textSize="30sp"
            app:layout_constraintBottom_toTopOf="@+id/escape_btn"
            app:layout_constraintEnd_toEndOf="@+id/one_btn"
            app:layout_constraintStart_toStartOf="@+id/one_btn"
            app:layout_constraintTop_toBottomOf="@+id/one_btn" />

        <TextView
            android:id="@+id/seven_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/accentdark_border_table"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:tag="7"
            android:text="7"
            android:textColor="@color/background_light"
            android:textSize="30sp"
            app:layout_constraintBottom_toBottomOf="@+id/six_btn"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintStart_toStartOf="@+id/two_btn"
            app:layout_constraintTop_toTopOf="@+id/six_btn" />

        <TextView
            android:id="@+id/height_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/accentdark_border_table"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:tag="8"
            android:text="8"
            android:textColor="@color/background_light"
            android:textSize="30sp"
            app:layout_constraintBottom_toBottomOf="@+id/seven_btn"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintStart_toStartOf="@+id/three_btn"
            app:layout_constraintTop_toTopOf="@+id/seven_btn" />

        <TextView
            android:id="@+id/nine_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/accentdark_border_table"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:tag="9"
            android:text="9"
            android:textColor="@color/background_light"
            android:textSize="30sp"
            app:layout_constraintBottom_toBottomOf="@+id/height_btn"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintStart_toStartOf="@+id/four_btn"
            app:layout_constraintTop_toTopOf="@+id/height_btn" />

        <TextView
            android:id="@+id/zero_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="19dp"
            android:layout_marginLeft="19dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:background="@drawable/accentdark_border_table"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:tag="10"
            android:text="10"
            android:textColor="@color/background_light"
            android:textSize="30sp"
            app:layout_constraintBottom_toBottomOf="@+id/nine_btn"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/nine_btn"
            app:layout_constraintTop_toTopOf="@+id/nine_btn" />

        <TextView
            android:id="@+id/escape_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/accentdark_border_table"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:tag="11"
            android:textColor="@color/background_light"
            android:textSize="50sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/nine_btn"
            app:layout_constraintHorizontal_bias="0.474"
            app:layout_constraintStart_toStartOf="@+id/seven_btn"
            app:layout_constraintTop_toBottomOf="@+id/six_btn" />

        <ImageView
            android:id="@+id/escape_logo"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="24dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginBottom="24dp"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:src="@mipmap/baseline_reply_white_24"
            android:tag="11"
            app:layout_constraintBottom_toBottomOf="@+id/escape_btn"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@+id/escape_btn"
            app:layout_constraintStart_toStartOf="@+id/escape_btn"
            app:layout_constraintTop_toTopOf="@+id/escape_btn" />

    </android.support.constraint.ConstraintLayout>

</FrameLayout>