<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.fmeimanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".controllers.activities.About"
            android:label="@string/About_title"/>
        <activity android:name=".controllers.activities.Help"
            android:label="@string/Help_title"/>
        <activity android:name=".controllers.activities.Settings"
            android:label="@string/Settings_title"/>
        <activity android:name=".controllers.activities.ExportDatas"
            android:label="@string/Export_datas_title"/>
        <activity
            android:name=".controllers.activities.Profile"
            android:label="@string/profile_title"
            android:parentActivityName=".controllers.activities.ProfileSection">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".controllers.activities.ProfileSection" />
        </activity>
        <activity
            android:name=".controllers.activities.ProfileSection"
            android:label="@string/profile_section_title" />
        <activity
            android:name=".controllers.activities.TeamFmeiBuilder"
            android:label="@string/team_fmei_builder_title"
            android:parentActivityName=".controllers.activities.TeamFmei">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".controllers.activities.TeamFmei" />
        </activity>
        <activity
            android:name=".controllers.activities.TeamFmei"
            android:label="@string/team_fmei_dashboard_title" />
        <activity
            android:name=".controllers.activities.CorrectiveAction"
            android:label="@string/Risk_file_corrective_action_title"
            android:parentActivityName=".controllers.activities.ProcessDashboard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".controllers.activities.ProcessDashboard" />
        </activity>
        <activity
            android:name=".controllers.activities.RiskFile"
            android:label="@string/Risk_file_description_title"
            android:parentActivityName=".controllers.activities.ProcessDashboard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".controllers.activities.ProcessDashboard" />
        </activity>
        <activity
            android:name=".controllers.activities.ProcessDashboard"
            android:label="@string/Process_dashboard_title"
            android:parentActivityName=".controllers.activities.FmeiDashboard">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".controllers.activities.FmeiDashboard" />
        </activity>
        <activity
            android:name=".controllers.activities.FmeiDashboard"
            android:label="@string/Fmei_dashboard_title" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>